{
  "name": "@sensoro-design/react",
  "version": "1.2.0",
  "description": "Sensoro Design React UI Library.",
  "module": "./es/index.js",
  "main": "./lib/index.js",
  "types": "./es/index.d.ts",
  "scripts": {
    "start": "dumi dev",
    "site": "dumi build",
    "build:dist": "arco-scripts build:component:dist",
    "build:dist:dev": "cross-env BUILD_ENV_MODE=development BUILD_ENV_DIST_FILENAME_JS=arco.development.js arco-scripts build:component:dist",
    "build:icon": "arco-scripts build:icon",
    "build:es": "arco-scripts build:component:es",
    "build:cjs": "arco-scripts build:component:cjs",
    "build:css": "arco-scripts build:component:css",
    "build:css:dev": "cross-env BUILD_ENV_MODE=development BUILD_ENV_DIST_FILENAME_CSS=arco.css arco-scripts build:component:css",
    "build:css:vars": "node ./scripts/css-variables.js",
    "build": "arco-scripts build:component && yarn build:dist:dev && yarn build:css:dev",
    "eslint": "eslint tests/ components/ --fix --cache --quiet --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint '{components,site}/**/*.less'",
    "test:client": "arco-scripts test:client",
    "test:node": "arco-scripts test:node --silent=false",
    "test": "cross-env NODE_ENV=test npm-run-all build:cjs test:client test:node",
    "color": "node ./scripts/compileColors.js",
    "format": "prettier --config .prettierrc --write \"{stories,tests,components}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "clean:dts": "esno ./scripts/clean-dts.ts",
    "release": "semantic-release",
    "release:manual": "walrus release"
  },
  "keywords": [
    "sensoro",
    "SensoroDesign",
    "react",
    "ui",
    "component"
  ],
  "homepage": "https://design.sensoro.com",
  "bugs": {
    "url": "https://github.com/sensoro-design/sensoro-design/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sensoro-design/sensoro-design-next"
  },
  "author": "SensoroDesign Team",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "@arco-design/color": "^0.4.0",
    "@babel/runtime": "^7.17.8",
    "@pansy/classnames": "^1.0.1",
    "@pansy/react-hooks": "^0.9.4",
    "@sensoro-design/icons": "^1.3.1",
    "b-tween": "^0.3.3",
    "b-validate": "^1.3.4",
    "compute-scroll-into-view": "^1.0.17",
    "dayjs": "^1.11.0",
    "lodash": "^4.17.21",
    "number-precision": "^1.5.2",
    "react-focus-lock": "^2.8.1",
    "react-transition-group": "^4.4.2",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-into-view-if-needed": "2.2.29"
  },
  "devDependencies": {
    "@arco-design/arco-scripts": "^1.25.15",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.1",
    "@types/lodash": "^4.14.173",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-sortable-hoc": "^0.7.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@umijs/utils": "^3.5.21",
    "@walrus/cli": "^1.3.4",
    "@walrus/cli-utils": "^2.2.1",
    "@walrus/plugin-release": "^1.14.3",
    "commitlint": "^16.2.3",
    "conventional-changelog-gitmoji-config": "^1.4.3",
    "cross-env": "^7.0.3",
    "dumi": "1.1.40",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-tsdoc": "^0.2.14",
    "esno": "^0.14.1",
    "husky": "^7.0.4",
    "less": "^3.12.2",
    "lint-staged": "^12.3.7",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.2",
    "prettier": "^2.6.2",
    "react": "^16.13.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^15.1.3",
    "react-dom": "^16.13.0",
    "react-draggable": "^4.4.4",
    "react-easy-crop": "^4.1.4",
    "react-resizable": "^3.0.4",
    "react-sortable-hoc": "^2.0.0",
    "react-sticky": "^6.0.3",
    "react-test-renderer": "^16.7.0",
    "semantic-release": "^19.0.2",
    "semantic-release-config-gitmoji": "^1.2.4",
    "sinon": "^9.0.1",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.3"
  },
  "sideEffects": [
    "dist/**/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "_unpkg": true,
  "publishConfig": {
    "access": "public"
  }
}
